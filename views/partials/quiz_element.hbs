<div class="container">
    {{#each payload.quiz}}
        <div class="form-group">
            <label><h5><b>{{this.description}}</b></h5></label>
            <p><b>{{this.context}}</b>
            </p>
        </div>
        <input type="radio" id="id_q_{{this.id}}" name="q_{{this.id}}" value={{this.choice_a}}>
        <label for="male"><b>a)</b> {{this.choice_a}}</label><br>
        <input type="radio" id="id_q_{{this.id}}" name="q_{{this.id}}" value={{this.choice_b}}>
        <label for="female"><b>b)</b> {{this.choice_b}}</label><br>
        <input type="radio" id="id_q_{{this.id}}" name="q_{{this.id}}" value={{this.choice_c}}>
        <label for="other"><b>c)</b> {{this.choice_c}}</label><br>
        <input type="radio" id="id_q_{{this.id}}" name="q_{{this.id}}" value={{this.choice_d}}>
        <label for="other"><b>d)</b> {{this.choice_d}}</label><br>
    {{/each}}
</div>


