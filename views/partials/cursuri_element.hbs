<div class="row">
    {{#each payload.cursuri}}
        <div class="col-md-12 col-xl-4 ">
            <div class="card overflow-hidden  shadow-sm p-3">
                <div class="card-body ">
                    <h5 class="card-title mb-3">{{this.name}}</h5>
                    <p class="card-text">{{this.description}}</p>
                    <a href="/cursuri?id={{this.id}}" class="btn btn-orange">Incepe cursul</a>
                </div>
            </div>
        </div>
    {{/each}}
</div>
